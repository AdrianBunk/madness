# Emacs should use -*- Makefile -*- mode.

MADNESS_VERSION = @MADNESS_VERSION@
MADNESS_MINOR_VERSION = @MADNESS_MINOR_VERSION@
MADNESS_MAJOR_VERSION = @MADNESS_MAJOR_VERSION@
BUILDID = @BUILDID@
CROSS_COMPILING = @CROSS_COMPILING@

# The object code suffix
OBJSUF = @OBJSUF@

# The library suffix
LIBSUF = @LIBSUF@

ENABLESHARED = no

LIBTOOL= 
LTLINK	 = 
LTLINKLIBOPTS 	= 
LTLINKBINOPTS = 	
LTCOMP = 	
LTINST = 

# The suffix generated by the -M compiler option
CXXDEPENDSUF = @CXXDEPENDSUF@
CXXDEPENDFLAGS = @CXXDEPENDFLAGS@
CCDEPENDSUF = @CXXDEPENDSUF@
CCDEPENDFLAGS = @CXXDEPENDFLAGS@

CXX = @CXX@
CXXDEPEND = @CXX@
CC = @CC@
CCDEPEND = @CC@

AR = @AR@
ARFLAGS = @ARFLAGS@

LD = $(CXX)
LDFLAGS = @LDFLAGS@

INSTALL = @INSTALL@
INSTALLDIROPT = -d -m 0755
INSTALLLIBOPT = -m 0644
INSTALLBINOPT = -m 0755
INSTALLSCRIPTOPT = -m 0755

RANLIB = @RANLIB@
PERL = @PERL@
COMPRESS = @COMPRESS@
UNCOMPRESS = @UNCOMPRESS@
LN_S = @LN_S@
LISTLIBS= $(PERL) $(SRCTOPDIR)/config/listlibs.pl -DLIBSUF=$(LIBSUF)
F77_SYMBOL = @F77_SYMBOL@
MKF77SYM= $(PERL) $(SRCTOPDIR)/config/mkf77sym.pl
ASTYLE = @ASTYLE@
ASTYLEFLAGS = @ASTYLEFLAGS@

# first check for generated include files in the machine dependent directories
# and then for include files in the src directory
INCLUDE = -I$(TOPDIR)/include -I$(TOPDIR)/src/lib -I$(SRCTOPDIR)/include -I$(SRCTOPDIR)/src/lib -I$(SRCTOPDIR)/src/apps \
	  @INCLUDES@  @CPPFLAGS@
DEFINES = @DEFINES@

CPPFLAGS = $(DEFINES) $(INCLUDE) 

CXXFLAGS = @CXXFLAGS@
HIGHOPT_CXXFLAGS = "THIS IS NOT CONFIGURED"
NOALIAS_CXXFLAGS = "THIS IS NOT CONFIGURED"
CFLAGS = @CFLAGS@
HIGHOPT_CFLAGS = "THIS IS NOT CONFIGURED"
NOALIAS_CFLAGS = "THIS IS NOT CONFIGURED"

HAVE_PTHREADS = @HAVE_PTHREADS@

SYSLIBS = @LIBS@
DOXYGEN = @DOXYGEN@
PDFLATEX = @PDFLATEX@

